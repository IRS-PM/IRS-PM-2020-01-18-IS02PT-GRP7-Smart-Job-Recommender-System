package com.iss_mr_grp7_job_recommender.iss_mr_grp7_job_recommender;

import java.lang.Number;

rule "validateApplicantInfo"
	dialect "mvel"
	ruleflow-group "validateApplicantInfoRuleFlowGroup"
	when
		app : Applicant( zipCode <= "10000" )
	then
		ValidationErrorDO fact0 = new ValidationErrorDO();
		fact0.setError( "Zip code should be six characters" );
		insert( fact0 );
		System.out.println("Executed Rule: " + drools.getRule().getName() );
end
