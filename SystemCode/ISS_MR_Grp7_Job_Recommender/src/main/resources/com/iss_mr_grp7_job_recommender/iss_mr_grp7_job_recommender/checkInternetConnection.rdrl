package com.iss_mr_grp7_job_recommender.iss_mr_grp7_job_recommender;

import java.lang.Number;

rule "checkInternetConnection"
	dialect "mvel"
	ruleflow-group "checkInternetConnectionRuleFlowGroup"
	when
		$appWifi : Applicant( )
	then
		System.out.println("Executed Rule: " + drools.getRule().getName() );
		internetCheck $wifiObj = new internetCheck();
		insert( $wifiObj );
		$wifiObj.checkInternetConnection();
end
