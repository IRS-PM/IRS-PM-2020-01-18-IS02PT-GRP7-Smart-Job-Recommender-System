package com.iss_mr_grp7_job_recommender.iss_mr_grp7_job_recommender;

import java.lang.Number;

rule "recommendJobPostingsToApplicant"
	dialect "mvel"
	ruleflow-group "recommendJobPostingsToApplicantRuleFlowGroup"
	when
		$jobPostingObj : JobPosting( )
		$topJobChoiceCollection : JobListCollection( )
	then
		System.out.println("Executed Rule: " + drools.getRule().getName() );
		$topJobChoiceCollection.addJobPosting( $jobPostingObj );
		retract( $jobPostingObj );
end
